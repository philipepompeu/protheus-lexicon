{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "totvs_language_debug",
			"request": "launch",
			"name": "TOTVS Language Debug",
			"program": "${command:AskForProgramName}",
			"cwb": "${workspaceFolder}",
			"smartclientBin": "../totvs/bin/smartclient/smartclient.exe",
			"isMultiSession": true,
			"enableTableSync": true,
			"extendFeatures": {
				"charDetails": false
			}
		},
		{
			"type": "java",
			"name": "Spring Boot-ProtheusLexiconBackendApplication<protheus-lexicon-backend>",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"mainClass": "com.philipepompeu.protheus_lexicon_backend.ProtheusLexiconBackendApplication",
			"projectName": "protheus-lexicon-backend",
			"args": "",
			"envFile": "${workspaceFolder}/.env"
		}
	]
}